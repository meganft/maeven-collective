<div class="container">
  <table class="table table-striped">
  <thead>
  <h3 style="text-align:center; padding-bottom: 20px;">Your Search Results</h3>
</thead>
  <% @organizations.each do |organization| %>
  <tr>
  <th><h4 style="font-weight:200; padding-bottom:10px;"><%= link_to organization.name.capitalize, organization_path(organization) %></h4></th>
  <th><h4 style="font-weight:200; padding-bottom:10px;"><%= organization.website %></h4></th>
  <% end %>
</tr>
</table>
</div>
